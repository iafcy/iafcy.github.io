{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": "./src",
    "strictNullChecks": true,
    "allowJs": true,
    "paths": {
			"@components/*": ["components/*"],
			"@layouts/*": ["layouts/*"],
			"@assets/*": ["assets/*"],
			"@pages/*": ["pages/*"],
			"@content/*": ["content/*"]
		}
  }
}